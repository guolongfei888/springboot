<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.panshi.security05.mapper.SysUserRoleMapper">
    <select id="listByUserId" parameterType="int" resultType="com.panshi.security05.entity.SysUserRole">
        SELECT * FROM sys_user_role WHERE user_id = #{userId}
    </select>
    <insert id="insertUserRole" >
        insert into sys_user_role values (#{userId},#{roleId})
    </insert>
</mapper>