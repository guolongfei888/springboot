<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.panshi.security05.mapper.SysPermissionMapper">
    <select id="listByRoleId" parameterType="int" resultType="com.panshi.security05.entity.SysPermission">
        SELECT * FROM sys_permission WHERE role_id=#{roleId}
    </select>

</mapper>